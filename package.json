{
  "name": "rnPushCode",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watchAll --coverage",
    "clear-watch": "watchman watch-del-all && watchman shutdown-server",
    "ios": "react-native run-ios --simulator='iPhone 6s Plus'",
    "ios-build": "react-native run-ios --configuration Release",
    "android-clean": "cd android && ./gradlew clean",
    "android-build-debug": "cd android/ && ./gradlew assembleDebug",
    "android-release": "cd android && ./gradlew installRelease --no-daemon",
    "android": "react-native run-android",
    "lint": "eslint --fix --ext .js src App.js",
    "install:pod": "cd ios && pod install && cd ..",
    "postinstall": "jetify",
    "debug": "adb reverse tcp:9090 tcp:9090"
  },
  "dependencies": {
    "@react-native-community/cli-platform-android": "^8.0.5",
    "appcenter": "^4.4.5",
    "appcenter-analytics": "^4.4.5",
    "appcenter-crashes": "^4.4.5",
    "babel-eslint": "10.0.1",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "18.0.1",
    "eslint-loader": "3.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-native": "^3.8.1",
    "native-base": "2.13.8",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-vector-icons": "^8.1.0",
    "react-native-code-push": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "7.13.7",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^3.4.3",
    "@types/jest": "^26.0.20",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.9.0",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "eslint": "^7.18.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.59.0",
    "react-native-dotenv": "^0.2.0",
    "react-test-renderer": "16.13.1",
    "reactotron-react-native": "^4.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
